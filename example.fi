fn sum(a: Int, b: Int) : Int do
  a + b
end

fn hello : String do
  "World"
end

fn foo : List(Int) do
  [1, 2, 3]
end

fn rec : {foo: List(Int)} do
  {foo: [1, 2, 3]}
end

fn sum_ref : Fn(Int, Int -> Int) do
  &sum(Int, Int)
end

fn call_sum_ref : Int do
  sum_ref().(1, 2)
end

fn bool : Bool do
  true
end
